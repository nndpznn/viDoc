{
  "name": "vidoc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "bun pm cache rm && rm -rf .next",
    "clean:all": "rm -rf node_modules & rm bun.lock* & bun clean",
    "docs": "typedoc",
    "test": "jest --testPathPattern=src/.*?.unit.test.ts",
    "test:int": "jest --testPathPattern=src/.*?.int.test.ts",
    "test:all": "jest --testPathPattern=src/.*?.test.ts",
    "tc": "tsc --noEmit",
    "ci": "bun run test && bun run lint && bun run tc",
    "supa:login": "npx supabase login",
    "supa:types": "npx supabase gen types typescript --project-id hyjvoljjhtgnxxsdsxbh --schema public > src/types/supabase.database.types.ts",
    "supa:zod": "bun ts-to-zod src/types/supabase.database.custom.types.ts src/types/supabase.database.zod.types.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "@react-oauth/google": "^0.12.1",
    "@supabase/supabase-js": "^2.43.4",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "next": "^14.2.13",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "css-loader": "^7.1.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.3",
    "typescript": "^5"
  }
}
